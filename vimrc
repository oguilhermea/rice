syntax on
colorscheme zellner

set nocompatible
set number relativenumber
set ruler
set novisualbell
set noerrorbells
set encoding=utf-8
set showmode
set showcmd

set wrap
set textwidth=79
set formatoptions=tcqrn1
set tabstop=4
set shiftwidth=4
set softtabstop=4
set expandtab
set noshiftround

set backspace=indent,eol,start

set hidden
